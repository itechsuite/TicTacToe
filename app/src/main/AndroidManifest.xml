<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.itechsuite.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ico"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.android.itechsuite.tictactoe.MainMenuScreen"
            android:label="@string/app_name"
            android:theme="@style/TicStatus">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.itechsuite.tictactoe.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/TicStatus"></activity>
        <activity
            android:name="com.android.itechsuite.tictactoe.Views"
            android:label="@string/app_name"
            android:theme="@style/TicStatus"></activity>
        <activity android:name="com.android.itechsuite.tictactoe.FiveViews"></activity>
    </application>

</manifest>